start	<- EventFilter '.' { printf(".\n"); }

EventFilter                 <- ('SELECT' SelectClauses+)                             { printf("eventFilter\n"); }

SelectClauses               <- SelectClausesStatement                                { printf("Selectclausestatement\n"); }

SelectClausesStatement      <- (TypeDefinitionId '[-]'                               { printf("selectclausestatement interpretation\n"); }

                                #Variable Node
                                (QualifiedName 'ATTRIBUTE' AttributeId

                                /QualifiedName 'ATTRIBUTE' AttributeId)

                                ('[-]' IndexRange)?)







TypeDefinitionId            <-  'ns='[0-9]+':i='[0-9]+
QualifiedName               <-  '[0-9]+ :'Name
Name                        <-  '.'
AttributeId                 <-  [0-2]? [0-9]
IndexRange                  <-  '(([0-9]:)? [0-9])'
                                / '([0-9],[0-9])'
                                /'[0-9]:[0-9],[0-9]:[0-9]'
